{
  "expo": {
    "name": "SixSimpleDice",
    "slug": "SixSimpleDice",
    "version": "2.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/android/adaptive-icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "owner": "20boyerd",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.20boyerd.SixSimpleDice",
      "icon": {
        "dark": "./assets/images/ios/dark-icon.png",
        "light": "./assets/images/ios/light-icon.png",
        "tinted": "./assets/images/ios/tinted-icon.png"
      }
    },
    "android": {
      "package": "com.boyerd.sixsimpledice",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/android/adaptive-icon.png"
      },
      "icon": "./assets/images/android/adaptive-icon.png"
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "f58c856d-8051-4ba3-925e-0e5da8d25920"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/f58c856d-8051-4ba3-925e-0e5da8d25920"
    }
  }
}